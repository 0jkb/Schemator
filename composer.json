{
    "name": "0jkb/schemator",
    "version": "1.0.1",
    "description": "A Laravel package to auto-generate models and resources based on database schema.",
    "type": "library",
    "keywords": ["schemator", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jkbcoder",
            "email": "jkbcoder@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/0jkb/schemator"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "doctrine/dbal": ">=2.5",
        "illuminate/support": ">=5.1",
        "illuminate/database": ">=5.1",
        "illuminate/contracts": ">=5.1",
        "illuminate/filesystem": ">=5.1",
        "illuminate/console": ">=5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Jkbcoder\\Schemator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jkbcoder\\Schemator\\SchematorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
